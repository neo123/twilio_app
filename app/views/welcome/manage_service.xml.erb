<?xml version="1.0" encoding="UTF-8"?>
<Response>
<% if !@client.nil? %>
<Gather action="/welcome/place_order.xml?client_id=<%= xml_parser(@client.client_id) %>&amp;service_name=<%= xml_parser(@client.service_name)%>" numDigits=">=1" method="GET">
<Say>Please enter quantity.</Say>
</Gather>
<% else %>
<Gather action="/welcome/voice_reply.xml?client_id=<%= @client.client_id%>" numDigits="1" method="GET">
<Say>Please enter valid option.</Say>
<Say>Press 1 for try again.</Say>
<% end %>
<!-- If customer doesn't input anything, prompt and try again. -->
<Say>Sorry, I didn't get your response.</Say>
<Redirect method='GET'>/welcome/handle_wrong_input.xml?Digits=1</Redirect>
</Response>